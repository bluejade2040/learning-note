    "def merge(arr1,arr2):\n",
    "    p = 0\n",
    "    q = 0  #兩個arr的index\n",
    "    sort = []\n",
    "    a = len(arr1)+len(arr2)\n",
    "    for i in range(a):\n",
    "        if arr1[p] and arr2[q]: #把較小的append進sort[]\n",
    "            if arr1[p] < arr2[q]:\n",
    "                sort.append(arr1[p])\n",
    "                p = p+1\n",
    "            else:\n",
    "                sort.append(arr2[q])\n",
    "                q = q+1\n",
    "                \n",
    "    print(sort)\n",
    "merge([5,3,9,1],[2,0,4,8])  #試看看"
   ]
