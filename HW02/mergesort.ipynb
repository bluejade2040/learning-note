    "def mergesort(arr):\n",
    "    if len(arr) < 2:  #長度小於2，直接return\n",
    "        return arr\n",
    "    mid = len(arr)//2\n",
    "    arr1 = mergesort(arr[:mid])  #前半段\n",
    "    arr2 = mergesort(arr[mid:])  #後半段\n",
    "    return merge(arr1,arr2)"
   ]
