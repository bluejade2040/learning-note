    "def merge(arr1,arr2):\n",
    "    p = 0\n",
    "    q = 0  #兩個arr的index\n",
    "    sort = []\n",
    "     \n",
    "        \n",
    "    while p<len(arr1) and q<len(arr2):\n",
    "        if arr1[p] <= arr2[q]: #把較小的append進sort[]\n",
    "            sort.append(arr1[p])\n",
    "            p = p+1\n",
    "            \n",
    "        else:\n",
    "            sort.append(arr2[q])\n",
    "            q = q+1\n",
    "        \n",
    "    while p == len(arr1) and q<len(arr2):  #當arr1到底\n",
    "        sort.append(arr2[q])\n",
    "        q = q+1\n",
    "    while  q ==len(arr2) and p<len(arr1):  ##當arr2到底\n",
    "        sort.append(arr1[p])\n",
    "        p = p+1    \n",
    "       \n",
    "    print(sort)    \n",
    "merge([5,1,3,4],[2,-5,-9])  #試看看  "
   ]
